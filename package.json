{
  "name": "@metamask/eth-json-rpc-infura",
  "version": "8.1.2",
  "description": "json-rpc-engine middleware for Infura's endpoints",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/eth-json-rpc-infura.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rimraf dist && yarn build",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "prepublishOnly": "yarn build:clean && yarn lint && yarn test",
    "setup": "yarn install && yarn allow-scripts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@metamask/eth-json-rpc-provider": "^1.0.0",
    "@metamask/utils": "^5.0.1",
    "eth-rpc-errors": "^4.0.3",
    "json-rpc-engine": "^6.1.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^2.5.0",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.18.53",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "43.0.7",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^26.4.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.17",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^10.7.0",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false
    }
  }
}
